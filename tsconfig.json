{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",    "paths": {
      "@/*": ["./src/*"],
      "prometheos-client": ["./src/prometheos-client"],
      "@shared/api-client": ["./packages/shared-api-client/src"],
      "@shared/api-client/*": ["./packages/shared-api-client/src/*"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  }
}
